<?xml version="1.0" encoding="UTF-8"?>
<beans>
    <bean id="userDao" class="com.spring.tiny.test.bean.UserDao"/>

    <bean id="userService" class="com.spring.tiny.test.bean.UserService">
        <property name="company" value="腾讯"/>
        <property name="location" value="深圳"/>
        <property name="userDao" ref="userDao"/>
    </bean>

    <bean class="com.spring.tiny.test.common.MyBeanPostProcessor"/>
    <bean class="com.spring.tiny.test.common.MyBeanFactoryPostProcessor"/>
</beans>